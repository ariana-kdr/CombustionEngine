%P = W*w;                                                                    %Power of engine [W]

%n_W = abs(W/Q23);                                                           %Efficiency using work and heat intake
%n_Q = 1 - abs(Q45/Q23);                                                     %Efficiency using heat intake and outtake
%n_C = 1 - abs(Tref/T3);                                                     %Maximum carnot efficiency 

%}
