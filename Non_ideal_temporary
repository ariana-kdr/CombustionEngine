set(gca,'FontSize',18) 
title('P-v Diagram for the Real Cycle')
xlabel('Volume [m^3]')
ylabel('Pressure [bar]')

xlim([0 inf])
ylim([0 inf])

grid on

for p = 1:n_eth
plot(Results(p).Volume,Results(p).Pressure.*1e-5)
end

legend(100*eth + "% Ethanol")

hold off


